    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "mediasoup-client": "^3.6.43",
    "ts-jest": "^27.0.7",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
